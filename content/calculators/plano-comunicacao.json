{
  "name": "Plano de Comunicação",
  "type": "custom",
  "description": "Calculadora para orçamento de Plano de Comunicação baseado em tamanho da empresa e complexidade",
  "config": {
    "formula": "7200 * tamanhoEmpresa * complexidade",
    "variables": [
      {
        "name": "tamanhoEmpresa",
        "label": "Tamanho da Empresa",
        "type": "select",
        "defaultValue": "1",
        "options": [
          {
            "value": "1",
            "label": "Até 50 funcionários"
          },
          {
            "value": "1.3",
            "label": "51-200 funcionários"
          },
          {
            "value": "1.6",
            "label": "201-500 funcionários"
          },
          {
            "value": "2",
            "label": "Mais de 500 funcionários"
          }
        ]
      },
      {
        "name": "complexidade",
        "label": "Complexidade da Comunicação",
        "type": "select",
        "defaultValue": "1",
        "options": [
          {
            "value": "1",
            "label": "Simples"
          },
          {
            "value": "1.4",
            "label": "Moderada"
          },
          {
            "value": "1.8",
            "label": "Complexa"
          }
        ]
      }
    ],
    "hourlyRate": 150,
    "requiresLeadForm": true,
    "showPDF": true
  },
  "display": {
    "title": "Simule seu investimento",
    "resultLabel": "Investimento estimado:"
  }
}